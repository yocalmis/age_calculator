<?xml version='1.0' encoding='utf-8'?>
<widget id="io.yas" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ölüm Yaşı Hesapla</name>
    <description>
        Kaç yaşında öleceğinizi hesaplayın.
	
    </description>
    <author email="yocalmis@gmail.com" href="yocalmis@gmail.com">
        Yusuf Öcalmış
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
